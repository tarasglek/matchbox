{
  "id": "append-worker",
  "name": "Appends matchbox worker to simulate booting in cloud env",
  "boot": {
    "kernel": "/assets/coreos/1409.7.0/coreos_production_pxe.vmlinuz",
    "initrd": ["/assets/coreos/1409.7.0/coreos_production_pxe_image.cpio.gz"],
    "args": [
      "root=/dev/sda1",
      "coreos.config.url=http://my.docker.dnsmasq:8080/assets/ign/static/append-worker.ign",
      "coreos.first_boot=yes",
      "console=tty0",
      "console=ttyS0",
      "coreos.autologin"
    ]
  },
  "ignition_id": "append-worker.yaml"
}
